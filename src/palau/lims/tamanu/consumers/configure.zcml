<configure
    xmlns="http://namespaces.zope.org/zope"
    i18n_domain="palau.lims">

  <!-- Adapter to handle pushes -->
  <adapter
      name="tamanu.consumers.patient"
      factory=".patient.PatientPushConsumer"
      provides="senaite.jsonapi.interfaces.IPushConsumer"
      for="*"/>

  <adapter
      name="tamanu.consumers.analysisrequest"
      factory=".analysisrequest.AnalysisRequestPushConsumer"
      provides="senaite.jsonapi.interfaces.IPushConsumer"
      for="*"/>

</configure>
